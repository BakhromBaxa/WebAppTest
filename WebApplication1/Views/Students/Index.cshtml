@model IEnumerable<Student>
@{
    Layout = null
}

<form asp-asp-controller="Students" asp-action="Index" method="post" enctype="multipart/form-data">
    <input type="file" name="file" />
    <hr />
    <button type="submit">Upload</button>
</form>

@if (Model.Count() > 0)
{
    <hr />
    <table cellpadding="0" cellspacing="0" border="1">
        <tr>
            <td>username</td>
            <td>useridentifier</td>
            <td>age</td>
            <td>city</td>
            <td>phonenumber</td>
            <td>email</td>
        </tr>
        @foreach (Student student in Model)
        {
            <tr>
                <td>@student.username</td>
                <td>@student.useridentifier</td>
                <td>@student.age</td>
                <td>@student.city</td>
                <td>@student.phonenumber</td>
                <td>@student.email</td>
            </tr>
        }
    </table>
}